@model CJCProjectEstimatorMVC.Models.BaseViewModel
@{
    ViewBag.Title = "Login";
}

    @using (Html.BeginForm("Index", "Login"))
    {
        
          @Html.LabelFor(model => model.AppUserVM.UserName, "User Name:")
          @Html.TextBoxFor(model => model.AppUserVM.UserName, new { maxlength = 40 })
          @Html.LabelFor(model => model.AppUserVM.Password, "Password:")
          @Html.PasswordFor(model => model.AppUserVM.Password, new { maxlength = 15 })
          <input name="submit" type="submit" value="Login" id="Submit" />


    }
